{
  "name": "rest-utilitites",
  "version": "1.0.0",
  "description": "A library with some useful utilities for building a restful API.",
  "keywords": [
    "Rest",
    "Restful",
    "API"
  ],
  "main": "./dist/rest-utilities.umd.js",
  "module": "./dist/rest-utilities.es.js",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/rest-utilities.es.js",
      "require": "./dist/rest-utilities.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "private": false,
  "author": "Dominik Lippl <dominik.lippl@hotmail.de>",
  "repository": "https://github.com/dominiklippl/rest-utilities.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "tag": "latest"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "serve": "rollup -c ./rollup.config.mjs --watch",
    "build": "rollup -c ./rollup.config.mjs",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "tsc": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "rollup": "^3.15.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rxjs": "^7.8.0",
    "semantic-release": "^19.0.5",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
